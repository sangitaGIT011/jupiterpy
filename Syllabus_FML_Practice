      "cell_type": "code",
      "source": [
        "# Step 1: Import necessary libraries\n",
        "import pandas as pd\n",
        "import seaborn as sns\n",
        "from sklearn.model_selection import train_test_split\n",
        "from sklearn.linear_model import LogisticRegression\n",
        "from sklearn.metrics import accuracy_score\n",
        "from sklearn.preprocessing import LabelEncoder, MinMaxScaler\n",
        "\n",
        "# Step 2: Load real-world Titanic dataset from seaborn\n",
        "df = sns.load_dataset('titanic')\n",
        "\n",
        "# Step 3: Basic data info\n",
        "print(\"Original Data Sample:\")\n",
        "print(df.head())\n",
        "print(\"\\nMissing Values:\")\n",
        "print(df.isnull().sum())\n",
        "\n",
        "# Step 4: Handle missing values\n",
        "df['age'].fillna(df['age'].mean(), inplace=True)         # Fill age with average\n",
        "df.dropna(subset=['embarked'], inplace=True)             # Drop rows with missing 'embarked'\n",
        "df.drop(columns=['deck', 'embark_town', 'alive'], inplace=True)  # Drop unnecessary columns\n",
        "\n",
        "# Step 5: Convert categorical (text) columns to numbers\n",
        "label_encoder = LabelEncoder()\n",
        "for col in df.select_dtypes(include='object').columns:\n",
        "    df[col] = label_encoder.fit_transform(df[col])\n",
        "\n",
        "# Step 6: Normalize numeric columns\n",
        "scaler = MinMaxScaler()\n",
        "numeric_cols = df.select_dtypes(include=['int64', 'float64']).columns\n",
        "df[numeric_cols] = scaler.fit_transform(df[numeric_cols])\n",
        "\n",
        "# Step 7: Prepare data for ML\n",
        "X = df.drop('survived', axis=1)   # Features\n",
        "y = df['survived']                # Target (what we want to predict)\n",
        "\n",
        "# Step 8: Split into training and test sets (80% train, 20% test)\n",
        "X_train, X_test, y_train, y_test = train_test_split(X, y, test_size=0.2, random_state=0)\n",
        "\n",
        "# Step 9: Train a Machine Learning model (Logistic Regression)\n",
        "model = LogisticRegression()\n",
        "model.fit(X_train, y_train)\n",
        "\n",
        "# Step 10: Make predictions and evaluate\n",
        "y_pred = model.predict(X_test)\n",
        "accuracy = accuracy_score(y_test, y_pred)\n",
        "\n",
        "print(\"\\nâœ… Model Accuracy: {:.2f}%\".format(accuracy * 100))\n"
      ],
